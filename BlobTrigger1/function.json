{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "inblob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "samples-workitems/{name}.xlsx",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outblob",
      "type": "blob",
      "path": "samples-results/{name}.tsv",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}